<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="VcsFacade (Development)" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot">
    <option name="ACTIVE_PROFILES" value="ut, gitea" />
    <additionalParameters>
      <param>
        <option name="enabled" value="true" />
        <option name="name" value="spring.config.additional-location" />
        <option name="value" value="server/src/test/resources/" />
      </param>
    </additionalParameters>
    <module name="octopus-vcs-facade.vcs-facade.main" />
    <option name="SPRING_BOOT_MAIN_CLASS" value="org.octopusden.octopus.vcsfacade.VcsFacadeApplication" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="org.octopusden.octopus.vcsfacade.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <extension name="net.ashald.envfile">
      <option name="IS_ENABLED" value="true" />
